function FileExtraction(TarPath,TarFileName,TarFileNum,SrcFileName,SrcFileNum,destination)
for i = 1 : TarFileNum
    TarFilePath = strcat(TarPath,TarFileName{i},'.txt');
end
for i = 1 : TarFileNum
    for j = 1 : SrcFileNum
        if strcmp(TarFileName{i},SrcFileName{j})
            movefile()
        end
    end
end